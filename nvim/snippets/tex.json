{
	"Table": {
		"body": [
			"\\begin{table}[htbp]",
			"  \\centering",
			"  \\begin{tabular}{$3}\\toprule",
			"    $0",
			"  \\end{tabular}",
			"  \\caption[${1:caption}]{${1:caption}}\\label{tab:$2}",
			"\\end{table}"
		],
		"description": "my normal table",
		"prefix": "table"
	},

	"Julia": {
		"body": [
			"{\\large",
			"\\begin{jllisting}[language=julia, style=jlcodestyle]",
			"  $0",
			"\\end{jllisting}",
			"}"
		],
		"description": "Julia code block",
		"prefix": "julia"
	},

	"Figure": {
		"body": [
			"\\begin{figure}[htbp]",
			"  \\centering",
			"  \\includegraphics[width=1\\textwidth]{$3}",
			"  \\caption[${1:caption}]{${1:caption}}\\label{tab:$2}",
			"\\end{figure}$0"
		],
		"description": "my normal figure",
		"prefix": "figure"
	},

	"ThreeFigure": {
		"body": [
			"\\setlength{\\figwidth}{\\textwidth}",
			"\\begin{figure}[htbp]",
			"    \\centering",
			"    \\threesubfigures%",
			"    {$3}{fig:$4}{$5}",
			"    {$6}{fig:$7}{$8}",
			"    {$9}{fig:$10}{$11}",
			"    \\caption[${1:caption}]{${1:caption}}\\label{fig:$2}",
			"\\end{figure}$0"
		],
		"description": "three equal hight figures",
		"prefix": "figthree"
	},

	"TwoFigure": {
		"body": [
			"\\setlength{\\figwidth}{\\textwidth}",
			"\\begin{figure}[htbp]",
			"    \\centering",
			"    \\threesubfigures%{1}",
			"    {$3}{fig:$4}{$5}",
			"    {$6}{fig:$7}{$8}",
			"    \\caption[${1:caption}]{${1:caption}}\\label{fig:$2}",
			"\\end{figure}$0"
		],
		"description": "two equal hight figures",
		"prefix": "figtwo"
	}
}
